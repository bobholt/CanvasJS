{
  "validateIndentation": 2,
  "requireSpaceAfterKeywords": [
    "if",
    "else",
    "for",
    "while",
    "do",
    "switch",
    "return",
    "try",
    "catch"
  ],
  "maximumLineLength": 80,
  "validateQuoteMarks": "\"",
  "disallowTrailingWhitespace": true,
  "excludeFiles": [
    "web-server.js",
    "Cee.js",
    "Cee.min.js",
    "test/jasmine/**",
    "test/js-imagediff/**"
  ]
}